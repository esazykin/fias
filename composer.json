{
  "name": "vyuldashev/fias",
  "description": "ФИАС",
  "type": "library",
  "keywords": [
    "фиас",
    "fias",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Yuldashev",
      "email": "misterio92@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "graham-campbell/guzzle-factory": "^3.0",
    "illuminate/support": "~5.5.0|~5.6.0"
  },
  "autoload": {
    "psr-4": {
      "Vyuldashev\\Fias\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vyuldashev\\Fias\\FiasServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
